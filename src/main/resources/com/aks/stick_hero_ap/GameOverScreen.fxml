      <Label fx:id="bestScoreLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="20.0" layoutY="210.0" prefHeight="30.0" prefWidth="260.0" text="Best:12" textAlignment="CENTER">
         <font>
            <Font name="Arial Black" size="15.0" />
         </font>
      </Label>
      <Label fx:id="cherriesLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="20.0" layoutY="233.0" prefHeight="30.0" prefWidth="260.0" text="Cherries:10" textAlignment="CENTER">
         <font>
            <Font name="Arial Black" size="15.0" />
         </font>
      </Label>
   </children>
