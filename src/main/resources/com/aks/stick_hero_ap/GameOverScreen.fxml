      </Label>
      <Rectangle arcHeight="168.0" arcWidth="64.0" fill="#969696" height="62.0" layoutX="60.0" layoutY="300.0" opacity="0.7" stroke="BLACK" strokeType="INSIDE" width="180.0" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="71.0" layoutY="300.0" prefHeight="61.0" prefWidth="118.0" text="Revive - 5" textAlignment="CENTER">
         <font>
            <Font name="Arial Black" size="17.0" />
         </font>
      </Label>
      <ImageView fitHeight="38.0" fitWidth="41.0" layoutX="183.0" layoutY="312.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Cherry.png" />
         </image>
      </ImageView>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="50.0" layoutX="77.0" layoutY="400.0" opacity="0.4" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="50.0" />
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="50.0" layoutX="164.0" layoutY="400.0" opacity="0.4" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="50.0" />
      <ImageView fitHeight="50.0" fitWidth="50.0" layoutX="77.0" layoutY="400.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Home-Black.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="50.0" fitWidth="50.0" layoutX="164.0" layoutY="400.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Restart%20Button.png" />
         </image>
      </ImageView>
      <Button fx:id="reviveButton" layoutX="60.0" layoutY="300.0" mnemonicParsing="false" onAction="#switchToSinglePlayerReviveScreen" opacity="0.0" prefHeight="62.0" prefWidth="180.0" text="switchToGameScreen">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font></Button>
      <Button fx:id="homeScreenButton" layoutX="77.0" layoutY="400.0" mnemonicParsing="false" onAction="#switchToHomeScreen" opacity="0.0" prefHeight="50.0" prefWidth="50.0" text="homeScreen" />
